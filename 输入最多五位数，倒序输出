<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="" content="width=device-width, initial-scale=1.0">
    <title>js学习日记</title>
</head>

<body>
    <script>
        var num1 = Number(prompt("请输入五位正整数"))
        if (String(num1).length = 5) {
            for (i = 1; i <= 10000; i = i * 10) {
                var num2 = parseInt(num1 / i)
                var num3 = num2 % 10
                document.write(`${num3}`)
            }
        }
        if (String(num1).length != 5) {
            alert('不是一个五位正整数')
            location.reload()
        }
    </script>
</body>

</html>
